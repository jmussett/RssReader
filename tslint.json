{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": "never",
        "arrow-parens": [true, "ban-single-arg-parens"],
        "ordered-imports": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "no-any": true,
        "no-var-requires": false,
        "no-empty": false,
        "no-empty-interface": false
    },
    "rulesDirectory": []
}